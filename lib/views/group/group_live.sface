<main
  data-id="group"
  data-group-id={e(@group, :id, "")}
  class={
    "": !@current_user
  }
>
  <Dynamic.Component
    module={Config.get([:ui, :group, :sections, tab(@selected_tab)], Bonfire.UI.Social.ProfileTimelineLive)}
    {...assigns}
  >
    <:header>
      <Dynamic.Component
        module={Config.get(
          [:ui, :group, :hero_component],
          Bonfire.UI.Groups.GroupHeroLive
        )}
        group={@group}
        selected_tab={@selected_tab}
      />
    </:header>
  </Dynamic.Component>
</main>
